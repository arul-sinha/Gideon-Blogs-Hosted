#set_frame_container
    table-layout: auto
    margin-left: auto
    margin-right: auto
    width: 99%
    height: 40%

tr, td
    border: 3px solid red !important
    height: 100%

#set_frame_container_btns
    width: 10% 

#set_frame_container_settings
    width: 90%

#set_btn
    margin: 8px
    display: block
    background: none
    border: 2px red solid
    border-radius: 10px
    color: black
    transition: 1.5s
    width: 80%

#set_btn:hover
    border-radius: 0px
    color: red
    width: 90%
     

    .box{
    float:left;
    margin-right:20px;
}
.clear{
    clear:both;
}